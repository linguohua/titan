CREATE TABLE `node` (
    `id` int(20) unsigned NOT NULL AUTO_INCREMENT,
	`device_id` varchar(512) NOT NULL UNIQUE ,
    `last_time` varchar(225) DEFAULT NULL ,
    `create_time` varchar(225) DEFAULT NULL ,
    `geo` varchar(512),
	`node_type` varchar(255) DEFAULT '' ,
	`address` varchar(255) DEFAULT '' ,
    `is_online` int(20)  DEFAULT '0' ,
	`online_time` int(64) DEFAULT '0' ,
	`server_name` varchar(255) DEFAULT '' ,
	PRIMARY KEY (`id`)
  ) ENGINE=InnoDB AUTO_INCREMENT=1 DEFAULT CHARSET=utf8 COMMENT='node info';

CREATE TABLE `validate_result` (
    `id` int(20) unsigned NOT NULL AUTO_INCREMENT,
	`device_id` varchar(512) NOT NULL ,
	`validator_id` varchar(512) NOT NULL ,
    `round_id` varchar(225) NOT NULL ,
    `msg` varchar(512) ,
	`strat_time` varchar(255) DEFAULT '',
	`end_time` varchar(255) DEFAULT '',
	`server_name` varchar(255) DEFAULT '',
    `status` int(20)  DEFAULT '0' ,
	PRIMARY KEY (`id`)
  ) ENGINE=InnoDB AUTO_INCREMENT=1 DEFAULT CHARSET=utf8 COMMENT='validate result info';

CREATE TABLE `validate_err` (
    `id` int(20) unsigned NOT NULL AUTO_INCREMENT,
	`device_id` varchar(512) NOT NULL,
    `round_id` varchar(225) NOT NULL ,
	PRIMARY KEY (`id`)
  ) ENGINE=InnoDB AUTO_INCREMENT=1 DEFAULT CHARSET=utf8 COMMENT='validate error device';
  
CREATE TABLE `data_infos_cn_gd_shenzhen` (
	`cid` varchar(512) NOT NULL UNIQUE,
    `cache_ids` varchar(225) NOT NULL ,
    `status` int(20)  DEFAULT '0' ,
    `total_size` int(20)  DEFAULT '0' ,
	`reliability` int(64) DEFAULT '0' ,
	`need_reliability` int(64) DEFAULT '0' ,
    `cache_time` int(20)  DEFAULT '0' ,
	PRIMARY KEY (`cid`)
  ) ENGINE=InnoDB AUTO_INCREMENT=1 DEFAULT CHARSET=utf8 COMMENT='data infos';

CREATE TABLE `cache_infos_cn_gd_shenzhen` (
    `id` int(20) unsigned NOT NULL AUTO_INCREMENT,
	`cid` varchar(512) NOT NULL,
    `cache_id` varchar(225) NOT NULL ,
    `device_id` varchar(225) NOT NULL ,
    `status` int(20)  DEFAULT '0' ,
    `total_size` int(20)  DEFAULT '0' ,
	`reliability` int(64) DEFAULT '0' ,
	PRIMARY KEY (`id`)
  ) ENGINE=InnoDB AUTO_INCREMENT=1 DEFAULT CHARSET=utf8 COMMENT='cache infos';

CREATE TABLE `device_blocks_cn_gd_shenzhen` (
    `id` int(20) unsigned NOT NULL AUTO_INCREMENT,
	`cid` varchar(512) NOT NULL,
    `device_id` varchar(225) NOT NULL ,
    `fid` varchar(225) NOT NULL ,
    `carfile_id` varchar(225) ,
    `cache_id` varchar(225) ,
	PRIMARY KEY (`id`)
  ) ENGINE=InnoDB AUTO_INCREMENT=1 DEFAULT CHARSET=utf8 COMMENT='device blocks';

CREATE TABLE `block_devices_cn_gd_shenzhen` (
    `id` int(20) unsigned NOT NULL AUTO_INCREMENT,
	`cid` varchar(512) NOT NULL,
    `device_id` varchar(225) NOT NULL ,
	PRIMARY KEY (`id`)
  ) ENGINE=InnoDB AUTO_INCREMENT=1 DEFAULT CHARSET=utf8 COMMENT='block devices';
